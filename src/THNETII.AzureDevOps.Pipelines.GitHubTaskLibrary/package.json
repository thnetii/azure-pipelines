{
  "name": "thnetii.azuredevops.pipelines.githubtasklibrary",
  "version": "0.2.0",
  "description": "GitHub ServiceConnection Tasks Library",
  "scripts": {
    "initdev": "npm run-script initdev:npm",
    "initdev:npm": "npm run-script initdev:npm:base & npm run-script initdev:npm:tasks",
    "initdev:npm:base": "npm install",
    "initdev:npm:tasks": "npx glob-exec --foreach \"*/task.json\" -- \"cd {{file.dir}} && npm run-script initdev\"",
    "compile": "npm run-script compile:tasks",
    "compile:tasks": "npx glob-exec --foreach \"*/task.json\" -- \"cd {{file.dir}} && npm run-script compile\"",
    "package": "npm run-script compile && npm run-script package:tfx",
    "package:tfx": "npx tfx extension create --root . --output-path ../../bin --manifest-globs vss-extension.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thnetii/azure-pipelines.git"
  },
  "author": {
    "name": "TH-NETII Rasch Solutions",
    "url": "https://thnetii.td.org.uit.no"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/thnetii/azure-pipelines/issues"
  },
  "homepage": "https://github.com/thnetii/azure-pipelines#readme",
  "devDependencies": {
    "glob-exec": "^0.1.1",
    "tfx-cli": "^0.8.1"
  },
  "dependencies": {
    "thnetii.azuredevops.pipelines.githubtasklibrary.githubaccesstokentask": "file:githubaccesstokentask"
  }
}
