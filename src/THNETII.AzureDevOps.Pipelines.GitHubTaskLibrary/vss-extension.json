{
  "manifestVersion": 1,
  "id": "azuredevops-githubtasklibrary",
  "name": "GitHub ServiceConnection Tasks Library",
  "version": "0.2.0",
  "publisher": "thnetii",
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "description": "Azure Pipelines tasks for extracting the GitHub Access token from a Azure DevOps service connection.",
  "categories": [
    "Extension",
    "Marketplace",
    "GitHub",
    "Azure Pipelines"
  ],
  "icons": {
    "default": "images/icon.png"
  },
  "files": [
    {
      "path": "githubaccesstokentask"
    }
  ],
  "repository": {
    "type": "git",
    "uri": "https://github.com/thnetii/azure-pipelines"
  },
  "contributions": [
    {
      "id": "githubaccesstokentask",
      "type": "ms.vss-distributed-task.task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "name": "githubaccesstokentask"
      }
    }
  ]
}
