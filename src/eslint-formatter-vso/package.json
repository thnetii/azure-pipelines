  "module": "formatter.mjs",
  "files": [],
  "scripts": {
    "lint": "npm-run-all --continue-on-error lint:*",
    "lint:tsc": "tsc -p ./jsconfig.json",
    "lint:eslint": "eslint -c ./.eslintrc.yml . --ext=js,mjs,cjs",
    "vso:lint": "npm-run-all --continue-on-error vso:lint:*",
    "vso:lint:eslint": "node ./azure-pipelines-vso-eslint -c ./.eslintrc.yml . --ext=js,mjs,cjs",
    "vso:lint:tsc": "node ../azure-pipelines-vso-tsc -p ./jsconfig.json"
  },
  "dependencies": {
    "azure-pipelines-task-lib": "^3.2.0"
  },
  "devDependencies": {
    "@tsconfig/node16-strictest-esm": "^1.0.0",
    "@types/q": "^1.5.5",
    "eslint": "^8.2.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-node": "^11.1.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^4.5.5"
  },
  "peerDependencies": {
    "eslint": ">=7"
  }
}
